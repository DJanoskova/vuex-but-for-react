{
  "name": "vuex-but-for-react",
  "version": "1.0.2",
  "private": false,
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/DJanoskova/vuex-but-for-react"
  },
  "homepage": "https://github.com/DJanoskova/vuex-but-for-react#readme",
  "keywords": ["react", "reducer", "state", "store", "functional", "flux"],
  "dependencies": {
    "@types/react": "^16.8.0",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "react": "^16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf ./lib && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "publish": "yarn build && git add . && git commit -m 'build' && git push && npm publish"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "/lib"
  ]
}
